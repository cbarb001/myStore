<br><br><br>

<% cache ['store', Product.latest] do %>
  <% @products.each do |product| %>
    <% cache ['entry', product] do %>
      <%= product.title %><br>
      <%= product.description %><br>
      <%= product.price %><br>
      <%= image_tag product.avatar.url(:medium) %>
      
      <%= button_to 'Add to Cart', line_items_path(product_id: product) %><br>

    <% end %>
  <% end %>
<% end %>


